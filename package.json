{
  "name": "gatsby-tv",
  "description": "The transparent, distributed video sharing platform.",
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsby-tv/gatsby.git"
  },
  "version": "0.1.3",
  "license": "AGPL-3.0",
  "private": true,
  "scripts": {
    "dev": "yarn workspace gatsby run dev",
    "start": "yarn workspace gatsby run start",
    "build": "yarn workspaces foreach --topological-dev run build",
    "format": "yarn workspaces foreach run format"
  },
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "dependenciesMeta": {
    "open@7.4.0": {
      "unplugged": true
    }
  }
}

    "eslint-plugin-react-hooks": "^4.2.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^17.0.1",
    "react-lorem-ipsum": "^1.4.7",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.2.1",
    "typescript": "^4.0.5",
    "webpack": "^5.21.2"
  },
  "dependencies": {
    "@popperjs/core": "^2.5.4",
    "color": "^3.1.2",
    "core-js": "^3.7.0",
    "react-popper": "^2.2.4",
    "regenerator-runtime": "^0.13.7"
  }
